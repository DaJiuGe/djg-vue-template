{
  "cSpell.words": [
    "attributify",
    "axios",
    "iconify",
    "mockjs",
    "pinia",
    "pnpm",
    "stylelint",
    "typecheck",
    "Unocss",
    "unplugin",
    "Vite",
    "vitejs",
    "vitest",
    "windi",
    "windicss"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "files.associations": {
    "*.css": "postcss",
  },
  "editor.formatOnSave": true,
  "stylelint.enable": true,
  "stylelint.validate": [
    "css",
    "less",
    "postcss",
    "scss",
    "sass"
  ]
}